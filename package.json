{
  "name": "dhune",
  "productName": "Dhune Player",
  "version": "0.0.1",
  "description": "Music player",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=app/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Dhune Player\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=app/assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [
    "music",
    "player",
    "audio"
  ],
  "author": "Krishna Modi",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.8.0",
    "fs": "0.0.1-security"
  },
  "devDependencies": {
    "electron-packager": "^11.1.0"
  }
}